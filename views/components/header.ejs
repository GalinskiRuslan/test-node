<header>
  <div><p class="logo">LOGO</p></div>
  <ul class="nav-links">
    <li><a href="/">Home</a></li>
    <li><a href="/about">About</a></li>
    <li><a href="#">Gallery</a></li>
    <li><a href="#">Contacts</a></li>
    <li><a href="#">Docs</a></li>
    <button id="log-btn">Login</button>
  </ul>
</header>
<div class="wrapper" id="log-form">
  <span class="icon-close"
    ><svg
      xmlns="http://www.w3.org/2000/svg"
      class="ionicon"
      viewBox="0 0 512 512"
    >
      <path
        d="M289.94 256l95-95A24 24 0 00351 127l-95 95-95-95a24 24 0 00-34 34l95 95-95 95a24 24 0 1034 34l95-95 95 95a24 24 0 0034-34z"
      /></svg></span>
  <div class="form-box register">
    <label class="form-label">Registration</label>
    <form method="post" autocomplete="off" action="/user-registr">
      <div class="input-box">
        <span class="form-icon">
          <svg
            xmlns="http://www.w3.org/2000/svg"
            class="ionicon"
            viewBox="0 0 512 512"
          >
            <path
              d="M332.64 64.58C313.18 43.57 286 32 256 32c-30.16 0-57.43 11.5-76.8 32.38-19.58 21.11-29.12 49.8-26.88 80.78C156.76 206.28 203.27 256 256 256s99.16-49.71 103.67-110.82c2.27-30.7-7.33-59.33-27.03-80.6zM432 480H80a31 31 0 01-24.2-11.13c-6.5-7.77-9.12-18.38-7.18-29.11C57.06 392.94 83.4 353.61 124.8 326c36.78-24.51 83.37-38 131.2-38s94.42 13.5 131.2 38c41.4 27.6 67.74 66.93 76.18 113.75 1.94 10.73-.68 21.34-7.18 29.11A31 31 0 01432 480z"
            />
          </svg>
        </span>
        <input autocomplete="off" name="username" type="name" required />
        <label class="form-input__label">User Name</label>
      </div>
      <div class="input-box">
        <span class="form-icon">
          <svg
            xmlns="http://www.w3.org/2000/svg"
            class="ionicon"
            viewBox="0 0 512 512"
          >
            <path
              d="M424 80H88a56.06 56.06 0 00-56 56v240a56.06 56.06 0 0056 56h336a56.06 56.06 0 0056-56V136a56.06 56.06 0 00-56-56zm-14.18 92.63l-144 112a16 16 0 01-19.64 0l-144-112a16 16 0 1119.64-25.26L256 251.73l134.18-104.36a16 16 0 0119.64 25.26z"/></svg        ></span>
        <input autocomplete="off" type="email" name="email" required />
        <label class="form-input__label">You mail</label>
      </div>
      <div class="input-box">
        <span class="form-icon"
          ><svg
            xmlns="http://www.w3.org/2000/svg"
            class="ionicon"
            viewBox="0 0 512 512"
          >
            <path
              d="M368 192h-16v-80a96 96 0 10-192 0v80h-16a64.07 64.07 0 00-64 64v176a64.07 64.07 0 0064 64h224a64.07 64.07 0 0064-64V256a64.07 64.07 0 00-64-64zm-48 0H192v-80a64 64 0 11128 0z"/></svg></span>
        <input autocomplete="off" type="password" name="password" required />
        <label class="form-input__label">Create password</label>
      </div>
      <div class="agreement"><label><input type="checkbox" /> Agree with agreement</label></div>
      <button type="submit" class="submit-btn">Register</button>
      <p>Alredy have account? <a class="link-to-log">LOGIN</a></p>
    </form>
  </div>
  <div class="form-box login active">
    <label class="form-label">Login</label>
    <form method="post" autocomplete="off" action="/check-user">
      <div class="input-box">
        <span class="form-icon">
          <svg
            xmlns="http://www.w3.org/2000/svg"
            class="ionicon"
            viewBox="0 0 512 512"
          >
            <path
              d="M424 80H88a56.06 56.06 0 00-56 56v240a56.06 56.06 0 0056 56h336a56.06 56.06 0 0056-56V136a56.06 56.06 0 00-56-56zm-14.18 92.63l-144 112a16 16 0 01-19.64 0l-144-112a16 16 0 1119.64-25.26L256 251.73l134.18-104.36a16 16 0 0119.64 25.26z"
            /></svg></span>
        <input autocomplete="off" type="email" name="email" required  />
        <label class="form-input__label">Enter you mail</label>
      </div>
      <div class="input-box">
        <span class="form-icon"
          ><svg
            xmlns="http://www.w3.org/2000/svg"
            class="ionicon"
            viewBox="0 0 512 512"
          >
            <path
              d="M368 192h-16v-80a96 96 0 10-192 0v80h-16a64.07 64.07 0 00-64 64v176a64.07 64.07 0 0064 64h224a64.07 64.07 0 0064-64V256a64.07 64.07 0 00-64-64zm-48 0H192v-80a64 64 0 11128 0z"
            /></svg></span>
        <input autocomplete="off" type="password" name="password" required />
        <label class="form-input__label">Enter you password</label>
      </div>
      <div class="remember-form">
        <label><input type="checkbox" />Remember me</label>
        <a href="#">Forgot password?</a>
      </div>

      <button type="submit" class="submit-btn">Loggin</button>
      <p>Don't have account? <a class="link-to-reg" >REGISTER</a></p>
    </form>
  </div>
</div>
<script>    
    const wrapper = document.querySelector(".wrapper");
    const register = document.querySelector('.register'); 
    const login = document.querySelector('.login')
    const linkReg = document.querySelector('.link-to-reg');
    const linkLog = document.querySelector('.link-to-log');
    const logBtn = document.getElementById('log-btn');
    const closeBtn = document.querySelector(".icon-close");

    linkReg.addEventListener("click", () => {
        register.classList.add('active');
        login.classList.remove('active'); 
        wrapper.classList.add('reg');
    });
    linkLog.addEventListener("click", () => {        
        register.classList.remove('active');
        login.classList.add('active');
        wrapper.classList.remove('reg'); });
    register.addEventListener("click", () => console.log('registr'));
    closeBtn.addEventListener("click", () => wrapper.classList.remove('active'));


    logBtn.addEventListener("click", () => {
        wrapper.classList.add('active');
        console.log('log')});


    
    
</script>